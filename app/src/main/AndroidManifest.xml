<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.intermediatequizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".About" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity" />
        <activity android:name=".MainActivityLogin" />
        <activity android:name=".ShowdataListview" />
        <activity android:name=".MainActivityListView" />
        <activity android:name=".StartingScreenActivity" />
        <activity android:name=".EndQuiz" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>